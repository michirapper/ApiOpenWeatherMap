<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.CompararFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/textView_name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="61dp"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline4"
        app:layout_constraintBottom_toTopOf="@+id/textView_temperature1"
        app:layout_constraintEnd_toStartOf="@+id/imageView1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_Description1"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="16dp"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBottom_toTopOf="@+id/textView_feels_like1"
        app:layout_constraintEnd_toStartOf="@+id/textView_temperature2"
        app:layout_constraintStart_toStartOf="@+id/textView_tempMax1"
        app:layout_constraintTop_toBottomOf="@+id/textView_name1"
        android:text="No hay resultados" />

    <TextView
        android:id="@+id/textView_tempMax1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_tempMin1"
        app:layout_constraintEnd_toStartOf="@+id/textView_tempMin1"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Maxima: 25.0°C" />

    <TextView
        android:id="@+id/textView_feels_like1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="8dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
        app:layout_constraintStart_toEndOf="@+id/textView_temperature1"
        app:layout_constraintTop_toTopOf="@+id/textView_temperature1"
        tools:text="Feels like 20.4°C" />

    <TextView
        android:id="@+id/textView_humedad1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="83dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView_tempMin1"
        app:layout_constraintStart_toStartOf="@+id/textView_tempMin1"
        tools:text="Humedad: 20 %" />

    <TextView
        android:id="@+id/textView_temperature1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginBottom="19dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline4"
        app:layout_constraintBottom_toTopOf="@+id/textView_tempMax1"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="25.0°C" />

    <TextView
        android:id="@+id/textView_tempMin1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="19dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBottom_toTopOf="@+id/textView_humedad1"
        app:layout_constraintEnd_toStartOf="@+id/textView_tempMax2"
        app:layout_constraintStart_toEndOf="@+id/textView_tempMax1"
        tools:text="Minima: 25.0°C" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        app:layout_constraintBottom_toTopOf="@+id/textView_Description1"
        app:layout_constraintEnd_toEndOf="@+id/textView_tempMin1"
        app:layout_constraintStart_toEndOf="@+id/textView_humedad1"
        app:layout_constraintTop_toTopOf="@+id/textView_name1"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginBottom="11dp"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline4"
        app:layout_constraintBottom_toTopOf="@+id/textView_Description2"
        app:layout_constraintStart_toStartOf="@+id/textView_Description2" />

    <TextView
        android:id="@+id/textView_tempMin2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_tempMax2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_tempMax2"
        tools:text="Minima: 25.0°C" />

    <TextView
        android:id="@+id/textView_tempMax2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="5dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBottom_toBottomOf="@+id/textView_tempMin1"
        app:layout_constraintEnd_toStartOf="@+id/textView_tempMin2"
        app:layout_constraintStart_toEndOf="@+id/textView_tempMin1"
        tools:text="Maxima: 25.0°C" />

    <TextView
        android:id="@+id/textView_humedad2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="82dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="89dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintEnd_toEndOf="@+id/textView_tempMin2"
        app:layout_constraintStart_toStartOf="@+id/textView_tempMax2"
        app:layout_constraintTop_toBottomOf="@+id/textView_tempMin2"
        tools:text="Humedad: 20 %" />

    <TextView
        android:id="@+id/textView_feels_like2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="9dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Body1"
        app:layout_constraintEnd_toEndOf="@+id/textView_Description2"
        app:layout_constraintTop_toBottomOf="@+id/textView_Description2"
        tools:text="Feels like 20.4°C" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="64dp"
        android:layout_height="0dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/textView_Description2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView_name2"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_Description2"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="53dp"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
        app:layout_constraintBottom_toBottomOf="@+id/textView_temperature2"
        app:layout_constraintEnd_toEndOf="@+id/textView_tempMin2"
        app:layout_constraintStart_toEndOf="@+id/imageView1"
        app:layout_constraintTop_toTopOf="@+id/imageView1"
       android:text="No hay resultados" />

    <TextView
        android:id="@+id/textView_temperature2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="19dp"
        android:textAppearance="@style/TextAppearance.MaterialComponents.Headline4"
        app:layout_constraintBottom_toTopOf="@+id/textView_tempMax2"
        app:layout_constraintStart_toStartOf="@+id/textView_tempMax2"
        tools:text="25.0°C" />


</androidx.constraintlayout.widget.ConstraintLayout>